{
  "name": "kudos-board-api",
  "version": "0.0.1",
  "scripts": {
    "postinstall": "npx prisma generate",
    "dev": "node --env-file=.env --watch index.js",
    "start": "node index.js",
    "prisma:seed": "node prisma/seed.js",
    "prisma:init": "npx prisma migrate dev --name init",
    "prisma:reset": "npx prisma migrate reset --force",
    "resetdb": "npm run prisma:reset && npm run prisma:init && npm run prisma:seed",
    "psql": "scripts/psql.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ladrillo/kudos-board-api.git"
  },
  "author": "Gabriel Cabrejas",
  "dependencies": {
    "@prisma/client": "^6.7.0",
    "express": "^4.21.2"
  }
}
